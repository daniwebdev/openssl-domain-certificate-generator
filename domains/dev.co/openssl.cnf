
[ req ]
default_bits       = 2048
distinguished_name = req_distinguished_name
req_extensions     = req_ext
x509_extensions    = v3_req
prompt             = no

[ req_distinguished_name ]
C  = US
ST = State
L  = City
O  = Organization
OU = Organizational Unit
CN = *.dev.co

[ req_ext ]
subjectAltName = @alt_names

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = *.dev.co
DNS.2 = qerja.dev.co
DNS.3 = app.qerja.dev.co
DNS.4 = office.qerja.dev.co
DNS.5 = api.qerja.dev.co
